{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "./node_modules/gts"
    ],
    "ignorePatterns": [
        "build/**/*"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "extends": [
                    "plugin:prettier/recommended"
                ],
                "endOfLine": "auto",
                "tabWidth": 2,
                "useTabs": false,
                "singleQuote": true,
                "printWidth": 200
            }
        ],
        "node/no-unpublished-import": [
            "off"
        ],
        "@typescript-eslint/no-explicit-any": [
            "off"
        ]
    }
}