{
  "name": "miku-discordmusicbot",
  "version": "3.0.0",
  "description": "A discord bot that plays music from youtube and local files and can also apply ffmpeg audio effects. ",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./**/*.ts",
    "fix": "eslint ./**/*.ts --fix",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "mocha --require source-map-support/register ./build/**/*test.js",
    "posttest": "npm run lint",
    "coverage": "nyc npm test",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register build/miku"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Miku-Music-Bot/Miku-DiscordMusicBot.git"
  },
  "author": "Bwu1324",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Miku-Music-Bot/Miku-DiscordMusicBot/issues"
  },
  "homepage": "https://github.com/Miku-Music-Bot/Miku-DiscordMusicBot#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.0",
    "@types/node-ipc": "^9.2.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "gts": "^3.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "typed-emitter": "^2.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "events": "^3.3.0",
    "node-ipc": "^10.1.0",
    "sqlite3": "^5.1.4",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
